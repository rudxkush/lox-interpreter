// Fibonacci sequence using recursion
fun fib(n) {
  if (n <= 1) return n;
  return fib(n - 2) + fib(n - 1);
}

for (var i = 0; i < 20; i = i + 1) {
  print fib(i);
}

// Fibonacci using iteration (more efficient)
fun fibIter(n) {
  if (n <= 1) return n;
  
  var a = 0;
  var b = 1;
  
  for (var i = 2; i <= n; i = i + 1) {
    var temp = a + b;
    a = b;
    b = temp;
  }
  
  return b;
}

print "Iterative version:";
for (var i = 0; i < 20; i = i + 1) {
  print fibIter(i);
}